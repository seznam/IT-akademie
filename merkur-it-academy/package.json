{
  "name": "merkur-login",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "dev:server": "cross-env NODE_CONFIG_DIR='./server/config' NODE_ENV=development nodemon server/server.js --config nodemon.json",
    "test": "npm run test:all",
    "test:unit": "cross-env TEST_GROUP='unit' jest -c ./jest.config.js",
    "test:integration": "npm run build; cross-env TEST_GROUP='integration' NODE_CONFIG_DIR='./server/config' jest -c ./jest.config.js",
    "test:all": "npm run build; cross-env TEST_GROUP='all' NODE_CONFIG_DIR='./server/config' jest -c ./jest.config.js",
    "test:watch": "cross-env TEST_GROUP='unit' jest -c ./jest.config.js --watchAll",
    "lint": "eslint -c ./.eslintrc.js --ignore-path ./.prettierignore './**/*.{js,jsx}'",
    "dev": "cross-env NODE_ENV=development webpack --config ./webpack.config.js --watch",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack.config.js",
    "start": "cross-env NODE_CONFIG_DIR='./server/config' NODE_ENV=production node server/server.js",
    "storybook": "start-storybook -p 6006",
    "regression": "build-storybook --quiet && loki --requireReference --reactUri file:./storybook-static",
    "regression:test": "npm run regression -- test",
    "regression:approve": "npm run regression -- approve"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@merkur/core": "^0.27.1",
    "@merkur/integration": "^0.27.1",
    "@merkur/plugin-component": "^0.27.1",
    "@merkur/plugin-error": "^0.27.1",
    "@merkur/plugin-event-emitter": "^0.27.1",
    "@merkur/plugin-http-client": "^0.27.1",
    "abort-controller": "^3.0.0",
    "classnames": "^2.3.1",
    "compression": "^1.7.4",
    "config": "^3.3.6",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "easy-uid": "^1.0.2",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-static-gzip": "^2.1.1",
    "got": "^11.8.2",
    "helmet": "^3.23.3",
    "morgan": "^1.10.0",
    "preact": "^10.5.14",
    "preact-render-to-string": "5.1.19",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.14.5",
    "@merkur/tool-storybook": "^0.27.1",
    "@merkur/tool-webpack": "^0.27.1",
    "@merkur/tools": "^0.27.1",
