{
  "name": "merkur-login",
  "version": "0.0.1",
  "scripts": {
    "dev:server": "NODE_CONFIG_DIR='./server/config' NODE_ENV=development nodemon server/server.js --config nodemon.json",
    "test": "npm run test:all",
    "test:unit": "TEST_GROUP='unit' jest -c ./jest.config.js",
    "test:integration": "npm run build; TEST_GROUP='integration' NODE_CONFIG_DIR='./server/config' jest -c ./jest.config.js",
    "test:all": "npm run build; TEST_GROUP='all' NODE_CONFIG_DIR='./server/config' jest -c ./jest.config.js",
    "test:watch": "TEST_GROUP='unit' jest -c ./jest.config.js --watchAll",
    "lint": "eslint -c ./.eslintrc.js './**/*.{js,jsx}'",
    "dev": "NODE_ENV=development webpack --config ./webpack.config.js --watch",
    "build": "NODE_ENV=production webpack --config ./webpack.config.js",
    "start": "NODE_CONFIG_DIR='./server/config' NODE_ENV=production node server/server.js",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook --quiet",
    "regression": "npm run storybook:build && loki --requireReference --reactUri file:./storybook-static",
    "regression:test": "npm run regression -- test",
    "regression:approve": "npm run regression -- approve"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@merkur/core": "^0.28.0",
    "@merkur/integration": "^0.28.0",
    "@merkur/plugin-component": "^0.28.0",
    "@merkur/plugin-error": "^0.28.0",
    "@merkur/plugin-event-emitter": "^0.28.0",
    "@merkur/plugin-http-client": "^0.28.1",
    "abort-controller": "^3.0.0",
    "classnames": "^2.3.1",
    "compression": "^1.7.4",
    "config": "^3.3.7",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "easy-uid": "^2.0.2",
    "ejs": "^3.1.7",
    "express": "^4.17.3",
    "express-static-gzip": "^2.1.5",
    "got": "^11.8.2",
    "helmet": "^3.23.3",
    "morgan": "^1.10.0",
    "preact": "^10.7.1",
    "preact-render-to-string": "5.1.21",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.16.7",
    "@merkur/tool-storybook": "^0.28.0",
    "@merkur/tool-webpack": "^0.28.0",
    "@merkur/tools": "^0.28.0",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/builder-webpack5": "^6.4.22",
    "@storybook/manager-webpack5": "^6.4.22",
    "@storybook/preact": "^6.4.22",
    "enzyme": "3.11.0",
    "enzyme-adapter-preact-pure": "4.0.1",
    "enzyme-to-json": "3.6.2",
    "identity-obj-proxy": "^3.0.0",
    "loki": "^0.29.0",
    "nodemon": "^2.0.15",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
